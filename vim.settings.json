{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    },
    {
      "before": ["<C-y>"],
      "commands": ["redo"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "r"],
      "commands": ["redo"]
    },
    {
      "before": ["x"],
      "after": ["\"", "_", "x"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["D"],
      "after": ["\"", "_", "D"]
    },
    {
      "before": ["d", "d"],
      "after": ["\"", "_", "d", "d"]
    },
    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", ";"],
      "commands": [{ "command": "editor.action.showHover" }]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    },
    {
      "before": ["<leader>", "t"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
        }
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-c>": false,
    "<C-d>": false,
    "<C-f>": false,
    "<C-s>": false,
    "<C-w>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-y>": false,
    "<C-z>": false
  }
}
